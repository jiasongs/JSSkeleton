platform :ios, '9.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'JSSkeletonExample' do
  pod 'JSSkeleton', :path => '..'
  pod 'QMUIKit'
  pod 'Masonry'
  pod 'MLeaksFinder', :configurations => ['Debug']
  pod 'LookinServer', :configurations => ['Debug']
end

target 'JSSkeletonExampleMacOS' do
  pod 'JSSkeleton', :path => '..'
  pod 'QMUIKit'
  pod 'Masonry'
  pod 'MLeaksFinder', :configurations => ['Debug']
end

